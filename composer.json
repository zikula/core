{
    "name": "zikula/core",
    "homepage": "https://ziku.la/",
    "license": "LGPL-3.0-or-later",
    "description": "Zikula Application Framework",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Zikula Team",
            "homepage": "https://github.com/zikula/core/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zikula\\CoreBundle\\": "src/system/CoreBundle/src",
            "Zikula\\LegalBundle\\": "src/system/LegalBundle/src",
            "Zikula\\ThemeBundle\\": "src/system/ThemeBundle/src",
            "Zikula\\UsersBundle\\": "src/system/UsersBundle/src"
        },
        "files": [
            "src/Kernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zikula\\CoreBundle\\Tests\\": "src/system/CoreBundle/tests",
            "Zikula\\ThemeBundle\\Tests\\": "src/system/ThemeBundle/tests"
        }
    },
    "require": {
        "php": ">=8.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-xml": "*",
        "doctrine/doctrine-bundle": "^3",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "doctrine/orm": "^3",
        "easycorp/easyadmin-bundle": "^4.28",
        "egulias/email-validator": "^4",
        "friendsofsymfony/jsrouting-bundle": "^3",
        "liip/imagine-bundle": "^2",
        "nucleos/profile-bundle": "^2",
        "nucleos/user-bundle": "^4",
        "stof/doctrine-extensions-bundle": "^1",
        "symfony/asset": "^8.0",
        "symfony/browser-kit": "^8.0",
        "symfony/config": "^8.0",
        "symfony/debug-bundle": "^8.0",
        "symfony/doctrine-bridge": "^8.0",
        "symfony/dotenv": "^8.0",
        "symfony/expression-language": "^8.0",
        "symfony/finder": "^8.0",
        "symfony/flex": "^2",
        "symfony/form": "^8.0",
        "symfony/http-client": "^8.0",
        "symfony/lock": "^8.0",
        "symfony/mailer": "^8.0",
        "symfony/monolog-bundle": "^4",
        "symfony/polyfill-intl-messageformatter": "^1",
        "symfony/polyfill-uuid": "^1",
        "symfony/rate-limiter": "^8.0",
        "symfony/requirements-checker": "^2",
        "symfony/runtime": "^8.0",
        "symfony/security-bundle": "^8.0",
        "symfony/stopwatch": "^8.0",
        "symfony/string": "^8.0",
        "symfony/translation": "^8.0",
        "symfony/twig-bundle": "^8.0",
        "symfony/uid": "^8.0",
        "symfony/ux-translator": "^2",
        "symfony/validator": "^8.0",
        "symfony/web-link": "^8.0",
        "symfony/web-profiler-bundle": "^8.0",
        "symfony/workflow": "^8.0",
        "symfony/yaml": "^8.0",
        "twig/extra-bundle": "^3",
        "twig/intl-extra": "^3",
        "twig/string-extra": "^3",
        "vakata/jstree": "^3",
        "vich/uploader-bundle": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "symfony/phpunit-bridge": "^8"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*",
        "zikula/core-bundle": "self.version",
        "zikula/legal-bundle": "self.version",
        "zikula/theme-bundle": "self.version",
        "zikula/users-bundle": "self.version"
    },
    "scripts": {
        "auto-scripts": {
            "composer dump-autoload": "script",
            "cache:clear": "symfony-cmd",
            "assets:install --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.4"
        },
        "symfony-assets-install": "relative"
    }
}

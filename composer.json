{
    "name": "zikula/core",
    "homepage": "https://ziku.la/",
    "license": "LGPL-3.0-or-later",
    "description": "Zikula Application Framework",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Zikula\\": "src/system/"
        },
        "files": [
            "src/Kernel.php"
        ]
    },
    "authors": [
        {
            "name": "Zikula Team",
            "homepage": "https://github.com/zikula/core/graphs/contributors"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-xml": "*",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2",
        "easycorp/easyadmin-bundle": "^4.3",
        "egulias/email-validator": "^4",
        "friendsofsymfony/jsrouting-bundle": "^3",
        "liip/imagine-bundle": "^2",
        "nucleos/profile-bundle": "^2",
        "nucleos/user-bundle": "^3",
        "stof/doctrine-extensions-bundle": "^1",
        "symfony/asset": "^6.3",
        "symfony/browser-kit": "^6.3",
        "symfony/config": "^6.3",
        "symfony/debug-bundle": "^6.3",
        "symfony/doctrine-bridge": "^6.3",
        "symfony/dotenv": "^6.3",
        "symfony/expression-language": "^6.3",
        "symfony/finder": "^6.3",
        "symfony/flex": "^2",
        "symfony/form": "^6.3",
        "symfony/http-client": "^6.3",
        "symfony/lock": "^6.3",
        "symfony/mailer": "^6.3",
        "symfony/monolog-bundle": "^3",
        "symfony/polyfill-intl-messageformatter": "^1",
        "symfony/polyfill-uuid": "^1",
        "symfony/rate-limiter": "^6.3",
        "symfony/requirements-checker": "^2",
        "symfony/runtime": "^6.3",
        "symfony/security-bundle": "^6.3",
        "symfony/stopwatch": "^6.3",
        "symfony/string": "^6.3",
        "symfony/translation": "^6.3",
        "symfony/twig-bundle": "^6.3",
        "symfony/ux-translator": "^2.9",
        "symfony/validator": "^6.3",
        "symfony/web-link": "^6.3",
        "symfony/web-profiler-bundle": "^6.3",
        "symfony/workflow": "^6.3",
        "symfony/yaml": "^6.3",
        "twig/extra-bundle": "^3",
        "twig/intl-extra": "^3",
        "twig/string-extra": "^3",
        "vakata/jstree": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "symfony/maker-bundle": "^1",
        "symfony/phpunit-bridge": "^6"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "zikula/core-bundle": "self.version",
        "zikula/categories-bundle": "self.version",
        "zikula/legal-bundle": "self.version",
        "zikula/theme-bundle": "self.version",
        "zikula/users-bundle": "self.version"
    },
    "scripts": {
        "auto-scripts": {
            "composer dump-autoload": "script",
            "cache:clear": "symfony-cmd",
            "assets:install --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.3"
        },
        "symfony-assets-install": "relative"
    }
}

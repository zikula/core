{
    "name": "gedmo/doctrine-extensions",
    "type": "library",
    "description": "Doctrine2 behavioral extensions",
    "keywords": [
        "behaviors",
        "doctrine2",
        "extensions",
        "gedmo",
        "sluggable",
        "loggable",
        "translatable",
        "tree",
        "nestedset",
        "sortable",
        "timestampable"
    ],
    "homepage": "http://gediminasm.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Gediminas Morkevicius",
            "email": "gediminas.morkevicius@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/common": ">=2.2,<2.5-dev"

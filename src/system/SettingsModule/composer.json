{
    "name": "zikula/settings-module",
    "version": "2.9.13",
    "description": "System settings administration",
    "type": "zikula-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Zikula",
            "homepage": "http://zikula.org/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\SettingsModule\\": "" }
    },
    "require": {
        "php": ">=5.5.9"

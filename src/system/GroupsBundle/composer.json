{
    "name": "zikula/groups-bundle",
    "version": "4.0.0-dev",
    "description": "User group administration",
    "type": "symfony-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\GroupsBundle\\": "" }
    },
    "require": {
        "php": "^8.1",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/form": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/mailer": "^5.4",
        "symfony/maker-bundle": "^1",
        "symfony/routing": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/validator": "^5.4",

        "zikula/core-bundle": "dev-3.1",
        "zikula/formextension-bundle": "dev-3.1",
        "zikula/extensions-bundle": "dev-3.1",
        "zikula/permissions-bundle": "dev-3.1",
        "zikula/theme-bundle": "dev-3.1",
        "zikula/users-bundle": "dev-3.1"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\GroupsBundle\\ZikulaGroupsModule",
            "displayname": "Groups",
            "url": "groups",
            "icon": "fas fa-users",
            "capabilities": {
                "admin": {
                    "route": "zikulagroupsbundle_group_adminlist"
                },
                "user": {
                    "route": "zikulagroupsbundle_group_listgroups"
                }
            },
            "securityschema": {
                "ZikulaGroupsModule::": "Group ID::",
                "ZikulaGroupsModule::memberslist": "::"
            }
        }
    }
}

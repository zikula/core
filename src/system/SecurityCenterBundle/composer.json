{
    "name": "zikula/securitycenter-bundle",
    "version": "4.0.0-dev",
    "description": "Security administration",
    "type": "symfony-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\SecurityCenterBundle\\": "" }
    },
    "require": {
        "php": "^8.1",
        "symfony/config": "^5.4",
        "symfony/contracts": "^2",
        "symfony/dependency-injection": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/filesystem": "^5.4",
        "symfony/form": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/mailer": "^5.4",
        "symfony/routing": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/string": "^5.4",
        "symfony/validator": "^5.4",

        "zikula/core-bundle": "dev-3.1",
        "zikula/formextension-bundle": "dev-3.1",
        "zikula/extensions-bundle": "dev-3.1",
        "zikula/menu-bundle": "dev-3.1",
        "zikula/permissions-bundle": "dev-3.1",
        "zikula/theme-bundle": "dev-3.1",
        "zikula/users-bundle": "dev-3.1",

        "ezyang/htmlpurifier": "dev-master"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\SecurityCenterBundle\\ZikulaSecurityCenterBundle",
            "displayname": "Security Center",
            "url": "securitycenter",
            "icon": "fas fa-shield-alt",
            "capabilities": {
                "admin": {
                    "route": "zikulasecuritycenterbundle_config_config"
                }
            },
            "securityschema": {
                "ZikulaSecurityCenterModule::": "::"
            }
        }
    }
}

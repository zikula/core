{
    "name": "zikula/extensions-bundle",
    "version": "4.0.0-dev",
    "description": "Extensions administration",
    "type": "symfony-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\ExtensionsBundle\\": "" }
    },
    "require": {
        "php": "^8.1",
        "symfony/contracts": "^2",
        "symfony/dependency-injection": "^5.4",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/string": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/yaml": "^5.4",

        "zikula/core-bundle": "dev-3.1",
        "zikula/formextension-bundle": "dev-3.1",
        "zikula/menu-bundle": "dev-3.1",
        "zikula/permissions-bundle": "dev-3.1",
        "zikula/theme-bundle": "dev-3.1",
        "zikula/sortable-columns": "1.*",

        "composer/semver": "^3",
        "justinrainbow/json-schema": "^5"
    },
    "extra": {
        "zikula": {
            "url": "extensions",
            "class": "Zikula\\ExtensionsBundle\\ZikulaExtensionsBundle",
            "displayname": "Extensions",
            "icon": "fas fa-plug",
            "capabilities": {
            },
            "securityschema": {
                "ZikulaExtensionsModule::": "::",
                "ZikulaExtensionsModule::modify": "extensionName::extensionsId"
            }
        }
    }
}

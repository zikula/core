{
  "name": "zikula/foomodule-bundle",
  "version": "2.0.0-beta",
  "description": "A foo module.",
  "type": "zikula-bundle",
  "license": ["MIT", "LGPL"],
  "authors": [
    {
      "name": "Zikula Team",
      "homepage": "https://ziku.la/",
      "email": "admin@example.com",
      "role": "lead"
    },
    {
      "name": "Zikula B Team",
      "homepage": "https://ziku.la/",
      "email": "admin@example.com",
      "role": "minion"

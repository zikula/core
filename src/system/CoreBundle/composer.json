    "bin": ["bin/console"],
    "require": {
        "php": ">=8.4",

        "symfony/config": "^8.0",
        "symfony/console": "^8.0",
        "symfony/finder": "^8.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/security-bundle": "^8.0",
        "symfony/string": "^8.0",
        "symfony/validator": "^8.0",

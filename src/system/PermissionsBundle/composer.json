{
    "name": "zikula/permissions-bundle",
    "version": "4.0.0-dev",
    "description": "User permissions manager",
    "type": "symfony-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\PermissionsBundle\\": "" }
    },
    "require": {
        "php": "^8.1",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "symfony/contracts": "^2",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/maker-bundle": "^1",
        "symfony/security-bundle": "^5.4",
        "symfony/validator": "^5.4",

        "zikula/core-bundle": "dev-3.1",
        "zikula/formextension-bundle": "dev-3.1",
        "zikula/extensions-bundle": "dev-3.1",
        "zikula/groups-bundle": "dev-3.1",
        "zikula/menu-bundle": "dev-3.1",
        "zikula/theme-bundle": "dev-3.1",
        "zikula/users-bundle": "dev-3.1"
    },
    "require-dev": {
        "symfony/translation": "^5.4"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\PermissionsBundle\\ZikulaPermissionsBundle",
            "displayname": "Permissions",
            "url": "permissions",
            "icon": "fas fa-key",
            "capabilities": {
                "admin": {
                    "route": "zikulapermissionsbundle_permission_listpermissions"
                }
            },
            "securityschema": {
                "ZikulaPermissionsModule::": "::"
            }
        }
    }
}

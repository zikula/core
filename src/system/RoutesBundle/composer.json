{
    "name": "zikula/routes-bundle",
    "version": "4.0.0-dev",
    "description": "Routes management",
    "type": "symfony-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Zikula contributors",
            "email": "info@ziku.la",
            "homepage": "https://ziku.la",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\RoutesBundle\\": "" }
    },
    "require": {
        "php": "^8.1",
        "symfony/config": "^5.4",
        "symfony/console": "^5.4",
        "symfony/contracts": "^2",
        "symfony/dependency-injection": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/filesystem": "^5.4",
        "symfony/form": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/mailer": "^5.4",
        "symfony/routing": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/string": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/workflow": "^5.4",

        "zikula/core-bundle": "dev-3.1",
        "zikula/formextension-bundle": "dev-3.1",
        "zikula/extensions-bundle": "dev-3.1",
        "zikula/groups-bundle": "dev-3.1",
        "zikula/menu-bundle": "dev-3.1",
        "zikula/pagelock-bundle": "1.*",
        "zikula/permissions-bundle": "dev-3.1",
        "zikula/settings-bundle": "dev-3.1",
        "zikula/theme-bundle": "dev-3.1",
        "zikula/users-bundle": "dev-3.1",
        "zikula/sortable-columns": "dev-main",

        "jms/i18n-routing-bundle": "dev-master",
        "friendsofsymfony/jsrouting-bundle": "^3"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\RoutesBundle\\ZikulaRoutesBundle",
            "displayname": "Routes",
            "url": "routes",
            "icon": "fas fa-map-marked-alt",
            "capabilities": {
                "admin": {
                    "route": "zikularoutesbundle_config_config"
                }
            },
            "securityschema": {
                "ZikulaRoutesModule::": "::",
                "ZikulaRoutesModule::Ajax": "::"
            }
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    }
}

{
    "name": "zikula/menu-module",
    "version": "3.1.0",
    "description": "Menu management",
    "type": "zikula-system-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\MenuModule\\": "" }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/config": "^5.4",
        "symfony/contracts": "^2.3",
        "symfony/dependency-injection": "^5.4",
        "symfony/doctrine-bridge": "^5.2",
        "symfony/event-dispatcher": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/finder": "^5.4",
        "symfony/form": "^5.4",
        "symfony/routing": "^5.4",
        "symfony/string": "^5.4",
        "symfony/validator": "^5.4",

        "zikula/core-bundle": "3.1.0",
        "zikula/blocks-module": "3.1.0",
        "zikula/extensions-module": "3.1.0",
        "zikula/permissions-module": "3.1.0",
        "zikula/theme-module": "3.1.0",

        "knplabs/knp-menu-bundle": "^3"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\MenuModule\\ZikulaMenuModule",
            "core-compatibility": ">=3.0.0",
            "displayname": "Menu Module",
            "url": "menu",
            "icon": "fas fa-compass",
            "capabilities": {
                "admin": {
                    "route": "zikulamenumodule_menu_listmenus"
                }
            },
            "securityschema": {
                "ZikulaMenuModule::": "::",
                "ZikulaMenuModule::id": "::id"
            }
        }
    }
}

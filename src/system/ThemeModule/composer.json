{
    "name": "zikula/theme-module",
    "version": "3.1.0-dev",
    "description": "Theme system and administration",
    "type": "zikula-system-module",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\ThemeModule\\": "" }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/asset": "^5.2",
        "symfony/config": "^5.2",
        "symfony/contracts": "^5.2",
        "symfony/cache": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/event-dispatcher": "^5.2",
        "symfony/filesystem": "^5.2",
        "symfony/finder": "^5.2",
        "symfony/form": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/routing": "^5.2",
        "symfony/string": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/yaml": "^5.2",
        "twig/cssinliner-extra": "3.*",
        "twig/html-extra": "3.*",
        "twig/inky-extra": "3.*",
        "twig/intl-extra": "3.*",
        "twig/markdown-extra": "3.*",
        "twig/string-extra": "3.*",
        "twig/extra-bundle": "3.*",

        "zikula/core-bundle": "dev-master",
        "zikula/coreinstaller-bundle": "dev-master",
        "zikula/hook-bundle": "dev-master",
        "zikula/blocks-module": "dev-master",
        "zikula/extensions-module": "dev-master",
        "zikula/menu-module": "dev-master",
        "zikula/permissions-module": "dev-master",
        "zikula/users-module": "dev-master",

        "zikula/bootstrap-theme": "dev-master",
        "zikula/default-theme": "dev-master",
        "zikula/rss-theme": "dev-master",
        "zikula/printer-theme": "dev-master",
        "zikula/atom-theme": "dev-master",

        "erusev/parsedown": "^1.7",
        "league/commonmark": "^1.0",
        "league/html-to-markdown": "^4.8",
        "michelf/php-markdown": "^1.9",

        "oro/twig-inspector": "1.*",

        "components/jquery": "3.*",
        "components/jqueryui": "1.*",
        "components/bootstrap": "4.*",
        "components/font-awesome": "5.*",
        "itsjavi/fontawesome-iconpicker": "3.*",
        "dimsemenov/magnific-popup": "1.*",
        "vakata/jstree": "3.*"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\ThemeModule\\ZikulaThemeModule",
            "core-compatibility": ">=3.0.0",
            "displayname": "Theme Module",
            "url": "theme",
            "icon": "fas fa-palette",
            "capabilities": {
                "admin": {
                    "route": "zikulathememodule_config_config"
                }
            }
        }
    }
}

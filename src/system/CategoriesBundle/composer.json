{
    "name": "zikula/categories-bundle",
    "version": "4.0.0-dev",
    "description": "Categories administration",
    "type": "symfony-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\CategoriesBundle\\": "" }
    },
    "require": {
        "php": "^8.1",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/doctrine-bridge": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/form": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/maker-bundle": "^1",
        "symfony/routing": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/validator": "^6.0",

        "zikula/core-bundle": "dev-3.1",
        "zikula/formextension-bundle": "dev-3.1",
        "zikula/theme-bundle": "dev-3.1",
        "zikula/users-bundle": "dev-3.1"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\CategoriesBundle\\ZikulaCategoriesBundle",
            "displayname": "Categories",
            "url": "categories",
            "icon": "fas fa-sitemap",
            "capabilities": {
                "admin": {
                    "route": "zikulacategoriesbundle_category_listcategories"
                }
            }
        }
    }
}

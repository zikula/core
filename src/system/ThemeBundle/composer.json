{
    "name": "zikula/theme-bundle",
    "version": "4.0.0-dev",
    "description": "Theme system and administration",
    "type": "symfony-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\ThemeBundle\\": "" }
    },
    "require": {
        "php": "^8.1",
        "symfony/asset": "^5.4",
        "symfony/config": "^5.4",
        "symfony/contracts": "^2",
        "symfony/cache": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/filesystem": "^5.4",
        "symfony/finder": "^5.4",
        "symfony/form": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/routing": "^5.4",
        "symfony/string": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/webpack-encore-bundle": "^1",
        "symfony/yaml": "^5.4",
        "twig/cssinliner-extra": "^3",
        "twig/html-extra": "^3",
        "twig/inky-extra": "^3",
        "twig/intl-extra": "^3",
        "twig/markdown-extra": "^3",
        "twig/string-extra": "^3",
        "twig/extra-bundle": "^3",

        "zikula/core-bundle": "dev-3.1",
        "zikula/extensions-bundle": "dev-3.1",
        "zikula/menu-bundle": "dev-3.1",
        "zikula/permissions-bundle": "dev-3.1",
        "zikula/users-bundle": "dev-3.1",

        "zikula/bootstrap-theme": "dev-3.1",
        "zikula/default-theme": "dev-3.1",
        "zikula/rss-theme": "dev-3.1",
        "zikula/printer-theme": "dev-3.1",
        "zikula/atom-theme": "dev-3.1",

        "erusev/parsedown": "^2",
        "league/commonmark": "^2",
        "league/html-to-markdown": "^5",
        "michelf/php-markdown": "^1",

        "oro/twig-inspector": "^1",

        "oomphinc/composer-installers-extender": "^2",
        "components/jquery": "^3",
        "components/jqueryui": "^1",
        "components/bootstrap": "^4",
        "components/font-awesome": "^6",
        "itsjavi/fontawesome-iconpicker": "^3",
        "dimsemenov/magnific-popup": "^1",
        "vakata/jstree": "^3"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\ThemeBundle\\ZikulaThemeBundle",
            "displayname": "Theme management",
            "url": "theme",
            "icon": "fas fa-palette",
            "capabilities": {
                "admin": {
                    "route": "zikulathemebundle_config_config"
                }
            }
        }
    }
}

{
    "name": "zikula/theme-bundle",
    "version": "4.0.0-dev",
    "description": "Theme system and administration",
    "type": "symfony-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\ThemeBundle\\": "" }
    },
    "require": {
        "php": "^8.1",
        "symfony/asset": "^5.4",
        "symfony/config": "^5.4",
        "symfony/cache": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/filesystem": "^5.4",
        "symfony/finder": "^5.4",
        "symfony/form": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/routing": "^5.4",
        "symfony/string": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/webpack-encore-bundle": "^1",
        "symfony/yaml": "^5.4",
        "twig/intl-extra": "^3",
        "twig/string-extra": "^3",
        "twig/extra-bundle": "^3",

        "zikula/core-bundle": "dev-3.1",
        "zikula/users-bundle": "dev-3.1",

        "knplabs/knp-menu-bundle": "^3",
        "oro/twig-inspector": "^1",

        "components/jquery": "^3",
        "itsjavi/fontawesome-iconpicker": "^3",
        "oomphinc/composer-installers-extender": "2.*",
        "vakata/jstree": "^3"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\ThemeBundle\\ZikulaThemeBundle",
            "displayname": "Theme management",
            "url": "theme",
            "icon": "fas fa-palette",
            "capabilities": {
                "admin": {
                    "route": "zikulathemebundle_branding_overview"
                }
            },
            "securityschema": {
                "LocaleSwitcher::": "::"
            }
        }
    }
}

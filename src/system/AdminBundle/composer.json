{
    "name": "zikula/admin-bundle",
    "version": "4.0.0-dev",
    "description": "Backend administration interface",
    "type": "symfony-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\AdminBundle\\": "" }
    },
    "require": {
        "php": "^8.1",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/form": "^5.4",
        "symfony/http-client": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/routing": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/validator": "^5.4",

        "zikula/core-bundle": "dev-3.1",
        "zikula/extensions-bundle": "dev-3.1",
        "zikula/formextension-bundle": "dev-3.1",
        "zikula/menu-bundle": "dev-3.1",
        "zikula/permissions-bundle": "dev-3.1",
        "zikula/theme-bundle": "dev-3.1",

        "frdh/mmenu.js": "^8"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/FrDH/mmenu-js"
        }
    ],
    "extra": {
        "zikula": {
            "class": "Zikula\\AdminBundle\\ZikulaAdminBundle",
            "displayname": "Administration panel",
            "url": "adminpanel",
            "icon": "fas fa-object-group",
            "capabilities": {
                "admin": {
                    "route": "zikulaadminbundle_admin_view"
                }
            },
            "securityschema": {
                "ZikulaAdminModule::": "::",
                "ZikulaAdminModule:Category:": "Admin Category name::"
            }
        }
    }
}

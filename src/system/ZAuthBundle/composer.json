{
    "name": "zikula/zauth-bundle",
    "version": "4.0.0-dev",
    "description": "Native zikula authentication",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Zikula Team",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\ZAuthBundle\\": "" }
    },
    "require": {
        "php": "^8.1",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "symfony/config": "^5.4",
        "symfony/console": "^5.4",
        "symfony/contracts": "^2",
        "symfony/dotenv": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/form": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/mailer": "^5.4",
        "symfony/maker-bundle": "^1",
        "symfony/rate-limiter": "^5.4",
        "symfony/routing": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/string": "^5.4",
        "symfony/validator": "^5.4",

        "zikula/core-bundle": "dev-3.1",
        "zikula/extensions-bundle": "dev-3.1",
        "zikula/groups-bundle": "dev-3.1",
        "zikula/menu-bundle": "dev-3.1",
        "zikula/permissions-bundle": "dev-3.1",
        "zikula/theme-bundle": "dev-3.1",
        "zikula/users-bundle": "dev-3.1",
        "zikula/sortable-columns": "dev-main",

        "ircmaxell/random-lib": "^1"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\ZAuthBundle\\ZikulaZAuthBundle",
            "displayname": "Zikula Native Authorization",
            "url": "zauth",
            "icon": "fas fa-user-lock",
            "capabilities": {
                "admin": {
                    "route": "zikulazauthbundle_useradministration_listmappings"
                }
            },
            "securityschema": {
                "ZikulaZAuthModule::": "::"
            }
        }
    }
}

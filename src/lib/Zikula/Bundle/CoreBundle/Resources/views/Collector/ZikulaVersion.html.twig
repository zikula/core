{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
    <img width="20" height="28" alt="Zikula Core" src="data:image/png;base64," />
    <span class="sf-toolbar-value">{{ collector.version }}</span>
    {% endset %}
    {% set text %}
    <div class="sf-toolbar-info-piece">
        <b>Zikula</b>
        {#<span>{{ collector.version }}</span>#}
    </div>
    <div class="sf-toolbar-info-piece">
        <b>Theme name</b>{{ collector.themeEngine.theme }}
    </div>
    <div class="sf-toolbar-info-piece">
        <b>Theme realm</b>{{ collector.themeEngine.realm }}
    </div>
    <div class="sf-toolbar-info-piece">
        <b>Theme annotation</b>{{ collector.themeEngine.annotation }}
    </div>
    <div class="sf-toolbar-info-piece">
        <a href="{{ collector.ghZikulaCoreUrl }}">GitHub Core Repository</a>
    </div>
    <div class="sf-toolbar-info-piece">
        <a href="{{ collector.ghZikulaDocsUrl }}">GitHub Documentation Repository</a>
    </div>
    <div class="sf-toolbar-info-piece">
        <a href="{{ collector.ghZikulaBootstrapDocsUrl }}">Styling Documentation</a>
    </div>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon"><img src="data:image/png;base64," alt="Zikula Core"></span>
        <strong>Zikula Core</strong>
        <span class="count">
            <span>{{ collector.version }}</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    <h2>Zikula Core {{ collector.version }}</h2>
    <p>@todo Add some useful information here.</p>
{% endblock %}

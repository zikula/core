{
    "name": "zikula/core-bundle",
    "type": "symfony-bundle",
    "description": "An bundle providing zikula-related symfony form extensions.",
    "keywords": [ "zikula" ],
    "homepage": "https://ziku.la",
    "license": "MIT",
    "authors": [
        {
            "name": "Zikula Team",
            "homepage": "https://ziku.la"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "composer/composer": "1.*",
        "composer/installers": "1.*",
        "robloach/component-installer": "*",

        "zikula/legal-module": "dev-master",
        "zikula/profile-module": "dev-master",
        "zikula/oauth-module": "dev-master",
        "zikula/pagelock-module": "dev-master",
        "zikula/andreas08-theme": "dev-master",
        "zikula/seabreeze-theme": "dev-master",
        "zikula/filesystem": "1.*",

        "symfony/symfony": "5.*",
        "symfony/monolog-bundle": "3.*",
        "sensio/framework-extra-bundle": "dev-master",
        "symfony/maker-bundle": "1.*",
        "symfony/profiler-pack": "^1.0",
        "elao/web-profiler-extra-bundle" : "2.*",
        "sensiolabs/security-checker" : "6.*",

        "prolix/translation-bundle": "1.*",
        "php-translation/symfony-bundle": "0.*",
        "nikic/php-parser": "4.*",
        "willdurand/js-translation-bundle": "v3.0.0",

        "doctrine/orm": "2.*",
        "doctrine/doctrine-bundle": "2.*",
        "antishov/doctrine-extensions-bundle": "1.*",
        "gedmo/doctrine-extensions": "2.*",

        "twig/twig": "3.*",
        "twig/cssinliner-extra": "3.*",
        "twig/html-extra": "3.*",
        "twig/inky-extra": "3.*",
        "twig/intl-extra": "3.*",
        "twig/markdown-extra": "3.*",
        "twig/string-extra": "3.*",
        "twig/extra-bundle": "3.*",

        "erusev/parsedown": "^1.7",
        "league/commonmark": "^1.0",
        "league/html-to-markdown": "^4.8",
        "michelf/php-markdown": "^1.9",

        "oyejorge/less.php": "dev-master",
        "liip/imagine-bundle": "dev-symfony5"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/zikula-modules/Legal"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zikula-modules/Profile"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zikula-modules/PageLock"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zikula/OAuth"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zikula/ZikulaAndreas08Theme"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zikula/ZikulaSeaBreezeTheme"
        },
        {
            "type": "vcs",
            "url": "https://github.com/prolixtechnikos/JMSTranslationBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/php-translation/symfony-bundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/vanputten/BazingaJsTranslationBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Guite/WebProfilerExtraBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/liip/LiipImagineBundle"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\Bundle\\CoreBundle\\": "" }
    }
}

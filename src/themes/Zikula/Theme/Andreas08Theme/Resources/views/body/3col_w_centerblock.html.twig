<div id="theme_content" class="clearfix">
    <div id="theme_leftcol" class="grid_3">
        {{ blockposition('left')|raw }}
    </div>
    <div id="theme_maincontent" class="grid_9">

        {{ blockposition('center')|raw }}
        {% block maincontent %}
        {% endblock %}
    </div>
    <div id="theme_rightcol" class="grid_4">
        {{ blockposition('right')|raw }}
    </div>
</div>
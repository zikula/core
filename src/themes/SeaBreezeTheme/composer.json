{
    "name": "zikula/seabreeze-theme",
    "version": "4.0.0",
    "description": "The SeaBreeze theme is a browser-oriented theme.",
    "type": "zikula-theme",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Zikula",
            "homepage": "http://zikula.org/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\SeaBreezeTheme\\": "" }
